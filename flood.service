[Unit]
Description=Flood
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
KillMode=process
#User=rtorrent
#PIDFile=/var/run/rtorrent.pid
#ExecStartPre=/bin/bash -c "if test -e /var/run/rtorrent/rtorrent.lock && test -z pidof rtorrent; then rm -f /var/run/rtorrent/rtorrent.lock; fi"
ExecStart=/usr/local/bin/flood
#WorkingDirectory=%h
Restart=on-failure

[Install]
WantedBy=multi-user.target
